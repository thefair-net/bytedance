    "require": {
        "php": ">=7.4",
        "ext-fileinfo": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "easywechat-composer/easywechat-composer": "^1.1",
        "guzzlehttp/guzzle": "^6.2 || ^7.0",
        "overtrue/socialite": "~2.0",
        "pimple/pimple": "^3.0",
        "psr/simple-cache": "^1.0",
